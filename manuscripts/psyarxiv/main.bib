@InProceedings{hoefermultiplicative2021,
  address	= {New York, NY, USA},
  title		= {The {Multiplicative} {Patient} and the {Clinical}
		  {Workflow}: {Clinician} {Perspectives} on {Social}
		  {Interfaces} for {Self}-{Tracking} and {Managing} {Bipolar}
		  {Disorder}},
